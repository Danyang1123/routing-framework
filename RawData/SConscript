p1 = Program(source=['CalibrateOsm.cpp'], LIBS=['routingkit'])
p2 = Program(source=['ComputeDijkstraRanks.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p3 = Program(source=['ComputeDistances.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p4 = Program(
    source=['ComputeUnionBoundary.cpp'], parse_flags='-DCSV_IO_NO_THREAD',
    LIBS=['CGAL', 'gmp', 'mpfr', 'proj'])
p5 = Program(
    source=['ConvertGraph.cpp'], parse_flags='-DCSV_IO_NO_THREAD', LIBS=['proj', 'routingkit'])
p6 = Program(source=['GenerateMatchingODPairs.cpp'], LIBS=['routingkit'])
p7 = Program(source=['GenerateODPairs.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p8 = Program(source=['ParseMobiTopp.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p9 = Program(source=['RadiationModel.cpp'], parse_flags='-DCSV_IO_NO_THREAD', LIBS=['proj'])

Alias('CalibrateOsm', p1)
Alias('ComputeDijkstraRanks', p2)
Alias('ComputeDistances', p3)
Alias('ComputeUnionBoundary', p4)
Alias('ConvertGraph', p5)
Alias('GenerateMatchingODPairs', p6)
Alias('GenerateODPairs', p7)
Alias('ParseMobiTopp', p8)
Alias('RadiationModel', p9)

Return('p1', 'p2', 'p3', 'p4', 'p5', 'p6', 'p7', 'p8', 'p9')
