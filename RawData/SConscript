p1 = Program(source=['ComputeDijkstraRanks.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p2 = Program(source=['ConvertGraph.cpp'], parse_flags='-DCSV_IO_NO_THREAD', LIBS=['proj'])
p3 = Program(source=['GenerateODPairs.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p4 = Program(source=['ParseMobiTopp.cpp'], parse_flags='-DCSV_IO_NO_THREAD')

Alias('ComputeDijkstraRanks', p1)
Alias('ConvertGraph', p2)
Alias('GenerateODPairs', p3)
Alias('ParseMobiTopp', p4)

Return('p1', 'p2', 'p3', 'p4')
