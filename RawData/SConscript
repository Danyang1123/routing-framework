p1 = Program(source=['ComputeDijkstraRanks.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p2 = Program(
    source=['ConvertGraph.cpp'], parse_flags='-DCSV_IO_NO_THREAD', LIBS=['proj', 'routingkit'])
p3 = Program(source=['GenerateMatchingODPairs.cpp'], LIBS=['routingkit'])
p4 = Program(source=['GenerateODPairs.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p5 = Program(source=['ParseMobiTopp.cpp'], parse_flags='-DCSV_IO_NO_THREAD')

Alias('ComputeDijkstraRanks', p1)
Alias('ConvertGraph', p2)
Alias('GenerateMatchingODPairs', p3)
Alias('GenerateODPairs', p4)
Alias('ParseMobiTopp', p5)

Return('p1', 'p2', 'p3', 'p4', 'p5')
